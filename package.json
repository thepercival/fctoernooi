{
  "name": "fctoernooi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "split-critical-css": "npm run split-critical-css-step1 && npm run split-critical-css-step2",
    "split-critical-css-step1": "cat dist/index.html | critical --base dist/ --inline > dist/index.critical.html",
    "split-critical-css-step2": "rm dist/index.html && mv dist/index.critical.html dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build-bundle-report && webpack-bundle-analyzer dist/stats.json",
    "build-bundle-report": "ng build --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.0.2",
    "@angular/core": "^13.0.2",
    "@angular/elements": "^13.0.2",
    "@angular/forms": "^13.0.2",
    "@angular/localize": "^13.0.2",
    "@angular/platform-browser": "^13.0.2",
    "@angular/platform-browser-dynamic": "^13.0.2",
    "@angular/router": "^13.0.2",
    "@angular/service-worker": "^13.0.2",
    "@fortawesome/angular-fontawesome": "^0.10.0",
    "@fortawesome/fontawesome-svg-core": "^6.0.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "angular-shepherd": "^13.0.1",
    "bootstrap": "^5.1.0",
    "lodash.clonedeep": "^4.5.0",
    "ngx-clipboard": "^15.0.0",
    "ngx-sport": "^1.0.21",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular/cli": "^13.0.3",
    "@angular/compiler-cli": "^13.0.2",
    "@angular/language-service": "^13.0.2",
    "@types/jasmine": "^3.8.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^14.17.10",
    "critical": "^4.0.1",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^4.0.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.4.4",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^4.0.0"
  }
