{
  "name": "fctoernooi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:nl": "ng serve --configuration=nl",
    "build": "ng build --prod && npm run split-critical-css",
    "build-acc": "ng build --configuration=acc && npm run split-critical-css",
    "build-dev": "ng build",
    "build-dev-nl": "ng build --configuration=production-nl",
    "split-critical-css": "npm run split-critical-css-step1 && npm run split-critical-css-step2",
    "split-critical-css-step1": "cat dist/index.html | critical --base dist/ --inline > dist/index.critical.html",
    "split-critical-css-step2": "rm dist/index.html && mv dist/index.critical.html dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build-bundle-report && webpack-bundle-analyzer dist/stats-es2015.json",
    "build-bundle-report": "ng build --prod --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/core": "^9.1.6",
    "@angular/forms": "^9.1.6",
    "@angular/localize": "^9.1.6",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@angular/router": "^9.1.6",
    "@angular/service-worker": "^9.1.6",
    "@fortawesome/angular-fontawesome": "0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.5",
    "lodash.clonedeep": "^4.5.0",
    "ngx-clipboard": "^13.0.1",
    "ngx-sport": "0.0.98",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.5",
    "@angular/cli": "^9.1.5",
    "@angular/compiler-cli": "^9.1.6",
    "@angular/language-service": "^9.1.6",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^3.1.1",
    "node-sass": "^4.14.1",
    "protractor": "^5.4.4",
    "ts-node": "~8.10.1",
    "typescript": "~3.8.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-dev-server": "^3.11.0",
    "critical": "^1.3.9"
  }
}