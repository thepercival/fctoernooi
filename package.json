{
  "name": "fctoernooi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:nl": "ng serve --configuration=nl",
    "build": "ng build --prod",
    "build-acc": "ng build --configuration=acc && npm run split-critical-css",
    "build-dev": "ng build",
    "build-dev-nl": "ng build --configuration=production-nl",
    "split-critical-css": "npm run split-critical-css-step1 && npm run split-critical-css-step2",
    "split-critical-css-step1": "cat dist/index.html | critical --base dist/ --inline > dist/index.critical.html",
    "split-critical-css-step2": "rm dist/index.html && mv dist/index.critical.html dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build-bundle-report && webpack-bundle-analyzer dist/stats.json",
    "build-bundle-report": "ng build --prod --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/forms": "^12.1.1",
    "@angular/localize": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@angular/service-worker": "^12.1.1",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0-beta.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "bootstrap": "^5.0.0",
    "lodash.clonedeep": "^4.5.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-sport": "^1.0.10",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.0",
    "@angular/cli": "^12.1.0",
    "@angular/compiler-cli": "^12.1.1",
    "@angular/language-service": "^12.1.1",
    "@types/jasmine": "^3.7.7",
    "@types/jasminewd2": "^2.0.9",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.4",
    "critical": "^4.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^4.0.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^3.11.2"
  }
}