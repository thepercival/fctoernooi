{
  "name": "fctoernooi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:nl": "ng serve --configuration=nl",
    "build": "ng build --prod",
    "build-acc": "ng build --configuration=acc && npm run split-critical-css",
    "build-dev": "ng build",
    "build-dev-nl": "ng build --configuration=production-nl",
    "split-critical-css": "npm run split-critical-css-step1 && npm run split-critical-css-step2",
    "split-critical-css-step1": "cat dist/index.html | critical --base dist/ --inline > dist/index.critical.html",
    "split-critical-css-step2": "rm dist/index.html && mv dist/index.critical.html dist/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build-bundle-report && webpack-bundle-analyzer dist/stats.json",
    "build-bundle-report": "ng build --prod --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^11.1.0",
    "@angular/compiler": "^11.1.0",
    "@angular/core": "^11.1.0",
    "@angular/forms": "^11.1.0",
    "@angular/localize": "^11.1.0",
    "@angular/platform-browser": "^11.1.0",
    "@angular/platform-browser-dynamic": "^11.1.0",
    "@angular/router": "^11.1.0",
    "@angular/service-worker": "^11.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@ng-bootstrap/ng-bootstrap": "^9.0.0",
    "bootstrap": "^4.6.0",
    "lodash.clonedeep": "^4.5.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-sport": "^1.0.10",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.0",
    "@angular/cli": "^11.1.0",
    "@angular/compiler-cli": "^11.1.0",
    "@angular/language-service": "^11.1.0",
    "@types/jasmine": "^3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "critical": "^2.0.6",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "^6.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^4.0.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}